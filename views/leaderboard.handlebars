<section>
  <h4 class="header">Leaderboard</h4>
  <table class="my_coolratings_table">
      <tr>
          <th>Position</th>
          <th>Username</th>
          <th>Number of Points</th>
          <th>Tier</th>
      </tr>
{{#each leaderboard}}
    <tr>
        <td>{{this.position}}</td>
        <td>{{this.name}}</td>
        <td>{{this.numOfPoints}}</td>
        <td>{{this.tier}}</td>
     </tr>
{{/each}}
</table>
</section>